<%@page contentType="text/html" pageEncoding="UTF-8"%>
<script type="text/javascript">
    function equalizeTable(tableID){
        document.getElementById(tableID).style.tableLayout='fixed';
    }
    function DeequalizeTable(tableID){
        document.getElementById(tableID).style.tableLayout=null;
    }
    function expandTable(tableID){
        document.getElementById(tableID).style.width="130%";
    }
    function contractTable(tableID){
        document.getElementById(tableID).style.width="100%";
    }

</script>
